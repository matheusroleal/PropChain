<%- contentFor('body') %>
<div class="row">
  <div class="col-md-12">
    <div class="card card-user">
      <div class="card-header">
        <h5 class="card-title">Search a Proposal</h5>
      </div>
      <div class="card-body">
        <form id="uploadbanner" enctype="multipart/form-data" method="post" action="/transactions/new">
          <div class="row">
            <div class="col-md-5 pr-1">
              <div class="form-group">
                <label>Search</label>
                <input type="text" id="search" name="search" class="form-control" placeholder="Proposal Id" value="">
                <div class="update ml-auto mr-auto">
                  <button value="submit" type="submit" class="btn btn-primary btn-round">Send</button>
                </div>
              </div>
            </div>
          </div>
        </form>
      </div>
    </div>
  </div>
</div>
<div class="row">
  <div class="col-md-12">
    <div class="card">
      <div class="card-body">
        <div class="table-responsive">
          <table class="table">
            <thead class=" text-primary">
              <th>Sender</th>
              <th>Receiver</th>
              <th>Data</th>
              <th class="text-right">Timestamp</th>
            </thead>
            <tbody>
              <tr>
                <td><%= transaction.sender %></td>
                <td><%= transaction.receiver %></td>
                <td><%= transaction.filepath %></td>
                <td class="text-right"><%= transaction.timestamp %></td>
                <td><button class="btn btn-primary btn-round" type="button" name="button">Open</button></td>
              </tr>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>
<%- contentFor('data') %>
